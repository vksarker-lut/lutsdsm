<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learnerkid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearnerKid">
        <activity
            android:name=".TestReportActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_test_report"
            android:theme="@style/Theme.LearnerKid.Fullscreen"></activity>
        <activity android:name=".TestActivity" />
        <activity android:name=".MathActivity" />
        <activity android:name=".FruitVegActivity" />
        <activity android:name=".AlphabetActivity" />
        <activity android:name=".MainActivity">
            android:screenOrientation="portrait"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>